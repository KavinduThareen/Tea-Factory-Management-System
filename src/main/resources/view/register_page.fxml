            <Font size="18.0" />
         </font></TextField>
        <TextField fx:id="contacTxt" layoutX="1353.0" layoutY="463.0" prefHeight="44.0" prefWidth="356.0" promptText="Contac" style="-fx-background-color: tra;">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="useridTxt" layoutX="1360.0" layoutY="364.0" prefHeight="51.0" prefWidth="347.0" promptText="User Id" style="-fx-background-color: tra;">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
    </children>
</AnchorPane>
